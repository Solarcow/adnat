<br>
<H3> Create Organisation </H3>

<%= form_for(@organisation) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>



    <%= f.label :name %>
    <%= f.text_field :name%>

    <%= f.label :rate, "Hourly Rate $" %>
    <%= f.number_field :rate%>

  <%= f.submit "Post", class: "btn btn-primary" %>
<% end %>